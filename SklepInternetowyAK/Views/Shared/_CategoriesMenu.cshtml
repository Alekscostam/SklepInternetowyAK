@model IEnumerable<SklepInternetowyAK.Models.Category>



<div id="menu">
    @foreach (var category in @Model)
    {
        <a style="margin: 20px; font-size: 2rem; color: azure; text-decoration: none" href="@Url.Action("List", "Films", new { categoryName = category.Name })">@category.Name</a>
    }

</div>